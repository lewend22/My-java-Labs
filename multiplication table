package lond0;

import java.util.Scanner;


public class Lond0 {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
          int number , result ;
    System.out.println("Enter a number  (to  multiplication table from 1 to 10) : ");
    number = in.nextInt();
  for(int i = 1;i<=10 ;i++){
   
       
       result = i * number ;
       
      System.out.println("result = " +number+"*" +i+  " = " +result);
   }
    }
    
}
